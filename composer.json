{
    "name": "mkamelmasoud/laravel-ads",
    "description": "laravel-ads-system",
    "keywords": ["laravel", "ads", "tags"],
    "authors": [{
        "name": "Mostafa Kamel Masoud",
        "email": "mostafakamel000@gmail.com"
    }],
    "license": "MIT",
    "type": "library",
    "require": {
        "php": "^7.4|^8.0",
        "spatie/laravel-tags": "^4.3",
        "codezero/laravel-unique-translation": "^3.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "MKamelMasoud\\Ads\\": "src/app/",
            "MKamelMasoud\\Ads\\Database\\Factories\\": "src/database/factories",
            "MKamelMasoud\\Ads\\Database\\Seeders\\": "src/database/seeders"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
